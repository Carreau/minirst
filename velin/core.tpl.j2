<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://fonts.xz.style/serve/inter.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@exampledev/new.css@1.1.2/new.min.css">
    <style>
          .foo:hover{text-decoration:underline}
          .foo{font-weight:bold; color:black}
          .exists{color:green;}
          .missing{color:red;}
          dd::before {
            content: '';
          }
          dd {
               margin-left: 40px;
          }
    </style>
</head>
<body>

    <header>
        <nav>/ pydata / {{module}} / {{version}} / api / {{qa}} </nav>
</header>

{% macro example(entries) -%}
    {{'>>> ' -}}
    {%- for index, type, text, t_ in entries %}
       {%- if text == '\n' -%}
           <br>...&nbsp;
        {%- else -%}
            {%- if t_ -%}
           <a class="foo" title={{t_}}, href="{{t_}}.html">{{text}}</a>
            {%- else -%}
            {{text}}
            {%- endif -%}
       {%- endif -%}
    {%- endfor-%}
    {{-'\n'}}
{%- endmacro %}


{% for section in doc.sections if doc[section] %}
    {% if section not in ['Extended Summary','Signature'] %}
        <h3>{{section}}</h3>
    {% endif %}
   {% if section in ['Summary', 'Extended Summary', 'Notes'] %}
       {%- for line in doc[section] %}{{ line }}{{'\n'}}{% endfor -%}
   {% elif section in ['Signature'] %}
       <code>{{doc[section]}}</code>
   {% elif section in ['See Also'] %}
       {% for r in doc[section] %}
            {{r}}
       {% endfor %}
   {% elif section in ['Examples'] %}
       {% for type,data in doc.edata %}
           {% if type=='text' %}
            <p>{{data}}</p>
           {% else %}
            <pre>{{example(data[0]) -}}
            {{- data[1]}}</pre>
           {% endif %}
            
       {% endfor %}
       See:
       {% for r in doc.refs %}
            <a class='{{exists(r)}}' href='{{r}}.html'>{{r}}</a>,
       {% endfor %}
   {% elif section in ['Parameters', 'Returns', 'Raises', 'Yields', 'Attributes'] %}

       <dl>
       {% for p in doc[section] %}
           <dt><code>{{p[0]}}</code>:{{p[1]}}</dt>
            <dd><p>{%- for line in p[2] -%}
               {{- line -}}
            {%- endfor -%}
            </p></dd>
       {% endfor %}

   </pre>
   {% else %}
       {% if doc[section] %}
        <pre>
       {{doc[section]}}
       </pre>
       {% endif %}
   {% endif %}
{% endfor%}
{% if doc.backrefs %}
    <h1>Back References</h1>
    {% for r in doc.backrefs %}
        <a class='{{exists(r)}}' href='{{r}}.html'>{{r}}</a>
    {% endfor %}
{% endif %}
</body>
</html>
